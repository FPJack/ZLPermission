{
  "name": "ZLPermission",
  "version": "0.1.0",
  "summary": "A short description of ZLPermission.",
  "description": "TODO: Add long description of the pod here.",
  "homepage": "https://github.com/FPJack/ZLPermission.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "fanpeng": "peng.fan@ukelink.com"
  },
  "source": {
    "git": "https://github.com/FPJack/ZLPermission.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "resource_bundles": {
    "ZLPermission_Rrivacy": [
      "ZLPermission/Resources/PrivacyInfo.xcprivacy"
    ]
  },
  "subspecs": [
    {
      "name": "base",
      "source_files": "ZLPermission/Classes/base/**/*"
    },
    {
      "name": "camera",
      "source_files": "ZLPermission/Classes/camera/**/*",
      "weak_frameworks": "AVFoundation"
    },
    {
      "name": "photo",
      "source_files": "ZLPermission/Classes/photo/**/*",
      "weak_frameworks": "Photos"
    },
    {
      "name": "microphone",
      "source_files": "ZLPermission/Classes/microphone/**/*",
      "weak_frameworks": "AVFoundation"
    },
    {
      "name": "location",
      "source_files": "ZLPermission/Classes/location/**/*",
      "weak_frameworks": "CoreLocation"
    },
    {
      "name": "bluetooth",
      "source_files": "ZLPermission/Classes/bluetooth/**/*",
      "weak_frameworks": "CoreBluetooth"
    },
    {
      "name": "calendar",
      "source_files": "ZLPermission/Classes/calendar/**/*",
      "weak_frameworks": "EventKit"
    },
    {
      "name": "reminders",
      "source_files": "ZLPermission/Classes/reminders/**/*",
      "weak_frameworks": "EventKit"
    },
    {
      "name": "tracking",
      "source_files": "ZLPermission/Classes/tracking/**/*"
    },
    {
      "name": "mediaLibrary",
      "source_files": "ZLPermission/Classes/mediaLibrary/**/*",
      "weak_frameworks": "MediaPlayer"
    },
    {
      "name": "notification",
      "source_files": "ZLPermission/Classes/notification/**/*",
      "weak_frameworks": [
        "UIKit",
        "UserNotifications"
      ]
    },
    {
      "name": "health",
      "source_files": "ZLPermission/Classes/health/**/*",
      "weak_frameworks": "HealthKit"
    },
    {
      "name": "contacts",
      "source_files": "ZLPermission/Classes/contacts/**/*",
      "weak_frameworks": "Accounts"
    },
    {
      "name": "siri",
      "source_files": "ZLPermission/Classes/siri/**/*",
      "weak_frameworks": "Intents"
    },
    {
      "name": "speechRecognition",
      "source_files": "ZLPermission/Classes/speechRecognition/**/*",
      "weak_frameworks": "Speech"
    },
    {
      "name": "motion",
      "source_files": "ZLPermission/Classes/motion/**/*",
      "weak_frameworks": "CoreMotion"
    }
  ]
}
